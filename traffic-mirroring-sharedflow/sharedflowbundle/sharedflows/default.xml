<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SharedFlow name="default">
  <Step>
    <Name>AM-SetTarget</Name>
    <Condition>(request-mirror-host != null) and (request-mirror-target != null) and (request-mirror-uri != null)</Condition>
  </Step>
  <Step>
    <Name>SC-RequestMirror</Name>
    <Condition>servicecallout.SC-RequestMirror.target.url != null</Condition>
  </Step>
</SharedFlow>
